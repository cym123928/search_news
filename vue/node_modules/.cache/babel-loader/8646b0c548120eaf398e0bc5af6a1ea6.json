ACA,QAAA,KAAI,CAAC,SAAL,GAAiB,GAAG,CAAC,IAAJ,CAAS,IAA1B;AACA,QAAA,KAAI,CAAC,KAAL,GAAa,GAAG,CAAC,IAAJ,CAAS,KAAtB;AACH,OAXD;AAYH,KAxBG;AA0BJ,IAAA,WA1BI,uBA0BQ,CA1BR,EA0BU;AAAA;;AAAI;AACd,MAAA,OAAO,CAAC,GAAR,CAAY,gBAAZ,EAA6B;AACzB,QAAA,MAAM,EAAC;AACH,UAAA,WAAW,EAAC,CAAC,CAAC;AADX;AADkB,OAA7B,EAIG,IAJH,CAIQ,UAAA,GAAG,EAAE;AACT,QAAA,MAAI,CAAC,UAAL,GAAkB,GAAG,CAAC,IAAtB;AACH,OAND;AAOH;AAlCG;AAfG,CAAf","sourcesContent":["<template>\r\n    <div style=\"padding:60px;margin-left: 2px\">\r\n        <!--stripe是斑马纹,sortable是排序-->\r\n        <el-table :data=\"tableData\" stripe border style=\"width: 100%\">\r\n            <!--<el-table-column prop=\"orderNumber\" label=\"订单号\" sortable width=\"50\">\r\n                <template #default=\"scope\">\r\n                    {{scope.row.orderNumber}}\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"number\" label=\"餐厅号\" sortable width=\"50\">\r\n                <template #default=\"scope\">\r\n                    {{scope.row.number}}\r\n                </template>\r\n            </el-table-column>-->\r\n            <el-table-column prop=\"name\" label=\"餐馆名称\"  width=\"100\">\r\n                <template #default=\"scope\">\r\n                    {{scope.row.name}}\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"city\" label=\"餐馆地址\" width=\"200\">\r\n                <template #default=\"scope\">\r\n                    {{scope.row.city}}\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"boss\" label=\"商家联系方式\" width=\"100\">\r\n                <template #default=\"scope\">\r\n                    {{scope.row.boss}}\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"price\" label=\"金额\" width=\"100\"  />\r\n            <el-table-column prop=\"orderTime\" label=\"下单时间\" width=\"150\"  />\r\n            <el-table-column prop=\"sendTime\" label=\"收货时间\" width=\"150\"  />\r\n            <el-table-column prop=\"address\" label=\"收货地址\" width=\"100\"  />\r\n            <el-table-column   width=\"100\">\r\n                <template #default=\"scope\">\r\n                    <el-button type=\"primary\" style=\"background-color:#F49506\" size=\"mini\" @click=\"orderDetail(scope.row);dialogVisible=true\">详情</el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n        <div style=\"margin: 10px 10px\">\r\n            <el-pagination\r\n                    v-model:currentPage=\"currentPage\"\r\n                    :page-sizes=\"[5,10,20]\"\r\n                    :page-size=\"pageSize\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\"\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n            >\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n\r\n    <div style=\"margin: 0px 50px\">\r\n        <el-dialog title=\"订单详情\" v-model=\"dialogVisible\" width=\"30%\">\r\n            <el-table :data=\"tableData3\">\r\n                <el-table-column\r\n                        label=\"点的菜品\"\r\n                        style=\"width: 200px\"\r\n                        prop=\"foodname\"\r\n                >\r\n                    <template #default=\"scope\">\r\n                        <span>{{scope.row.foodname}}</span>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n            <span style=\"margin: 0 80px\">\r\n                    <el-button type=\"primary\" style=\"background-color:#F49506\" @click=\"dialogVisible=false\">确 定</el-button>\r\n                    </span>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import request from \"../utils/request\";\r\n\r\n    export default {\r\n        name: \"\",\r\n        data(){\r\n            return{\r\n                currentPage:1,\r\n                pageSize:5,\r\n                total:10,\r\n                tableData: [],\r\n                tableData3:[],\r\n                dialogVisible:false\r\n            }\r\n        },\r\n        created() {\r\n             this.load();\r\n        },\r\n        methods:{\r\n            handleSizeChange(pageSize)\r\n            {\r\n                this.pageSize=pageSize\r\n                this.load()\r\n            },\r\n            handleCurrentChange(currentPage){\r\n                this.currentPage=currentPage;\r\n                this.load();\r\n            },\r\n\r\n            load(){\r\n                request.get('/api/OL/myOrdered',{\r\n                    params:{\r\n                        pageSize: this.pageSize,\r\n                        currentPage: this.currentPage,\r\n                        status:2,\r\n                        phone:sessionStorage.getItem(\"phone1\")\r\n                    }\r\n                }).then(res=>{\r\n                    console.log(\"/ol/myordered\")\r\n                    this.tableData = res.data.item\r\n                    this.total = res.data.total\r\n                })\r\n            },\r\n\r\n            orderDetail(e){   //显示订单详情  食物，顾客的手机号等等\r\n                request.get('/api/CR/findOD',{\r\n                    params:{\r\n                        orderNumber:e.orderNumber\r\n                    }\r\n                }).then(res=>{\r\n                    this.tableData3 = res.data\r\n                })\r\n            },\r\n\r\n\r\n        }\r\n    }\r\n</script>\r\n\r\n"],"sourceRoot":""}]}