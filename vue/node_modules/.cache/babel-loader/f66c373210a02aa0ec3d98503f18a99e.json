;;GAJJ,E;;AAAA,I,QAAA,C,EAkBA,mBAAA,CAWM,KAXN,EAAA,UAAA,EAWM,CAVF,YAAA,CASgB,wBAThB,EASgB;AARA,IAAA,WAAW,EAAE,KAAA,CAAA,WAQb;;aARa,KAAA,CAAA,W,GAAW,M;MAQxB;AAPP,kBAAY,CAAA,CAAA,EAAA,EAAA,EAAA,EAAA,CAOL;AANP,iBAAW,KAAA,CAAA,QAMJ;AALR,IAAA,MAAM,EAAC,yCAKC;AAJP,IAAA,KAAK,EAAE,KAAA,CAAA,KAIA;AAHP,IAAA,YAAW,EAAE,QAAA,CAAA,gBAGN;AAFP,IAAA,eAAc,EAAE,QAAA,CAAA;AAET,GAThB,E,IAAA,E;;AAAA,I,wEAAA,CAUE,CAXN,C","sourcesContent":["<template>\r\n    <el-page-header @back=\"goBack\" content=\"详情页面\"\r\n    style=\"margin-top: 30px;margin-left: 60px\"\r\n    >\r\n    </el-page-header>\r\n    <el-form :model=\"question\" label-position=\"left\">\r\n        <el-card style=\"width: 1000px;margin-top: 30px;margin-left: 250px;background-color: deepskyblue\"\r\n        shadow=\"never\"\r\n        >\r\n            <template #header>\r\n                <div class=\"card-header\">\r\n                    <span>发布人</span>\r\n                    <el-icon style=\"font-size: 20px\" class=\"el-icon-star-on\"><Star></Star></el-icon>\r\n                </div>\r\n            </template>\r\n            <div>问题详情</div>\r\n        </el-card>\r\n    <el-form-item style=\"margin-left: -50px;margin-top: 40px\">\r\n        <el-input prop=\"answer\" v-model=\"question.answer\"\r\n        style=\"width: 800px;height: 400px;margin-left: -25px\"\r\n        >\r\n        <template #suffix>\r\n            <el-icon style=\"font-size: 30px\" class=\"el-icon-chat-line-round\"><Chat-line-round></Chat-line-round></el-icon>\r\n        </template>\r\n        </el-input>\r\n        <el-button type=\"primary\" @click=\"load\">评论</el-button>\r\n    </el-form-item>\r\n    </el-form>\r\n    <el-table\r\n            :data=\"tableData\"\r\n            style=\"width: 1050px;margin-left: 250px;margin-top: -25%\"\r\n    >\r\n        <el-table-column\r\n                fixed\r\n                prop=\"orderAnswer\"\r\n                width=\"800\"\r\n        >\r\n        </el-table-column>\r\n        <el-table-column\r\n                fixed\r\n                prop=\"clicks\"\r\n                width=\"250\"\r\n        >\r\n            <el-icon class=\"el-icon-star-on\"><Star></Star></el-icon>\r\n        </el-table-column>\r\n    </el-table>\r\n    <div style=\"margin: 10px 10px\">\r\n        <el-pagination\r\n                v-model:currentPage=\"currentPage\"\r\n                :page-sizes=\"[5,10,20]\"\r\n                :page-size=\"pageSize\"\r\n                layout=\"total, sizes, prev, pager, next, jumper\"\r\n                :total=\"total\"\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n        >\r\n        </el-pagination>\r\n    </div>\r\n</template>\r\n    <script>\r\n        import {Star} from \"@element-plus/icons\";\r\n        import {ChatLineRound} from \"@element-plus/icons\";\r\n        import request from \"@/utils/request\";\r\n\r\n        export default {\r\n            components:{\r\n              Star,\r\n                ChatLineRound,\r\n            },\r\n            data(){\r\n              return{\r\n                  question:{\r\n                      answer:'',\r\n                      detail:''\r\n                  },\r\n                  tableData:[],\r\n                  currentPage:1,\r\n                  pageSize:5,\r\n                  total:10,\r\n              }\r\n            },\r\n            created(){\r\n              load()\r\n            },\r\n            methods: {\r\n                goBack() {\r\n                    this.$router.back();\r\n                },\r\n                load(){\r\n                    request.get('/api/question/findValued',{   //所有有效的问题\r\n                        params:{\r\n                            search: this.search,\r\n                            pageSize:this.pageSize,\r\n                            currentPage:this.currentPage,\r\n                        }\r\n                    }).then(res=>{\r\n                        this.tableData=res.data.item\r\n                        this.total=res.data.total\r\n                    })\r\n                },\r\n                handleSizeChange(pageSize)\r\n                {\r\n                    this.pageSize=pageSize\r\n                    this.load()\r\n                },\r\n                handleCurrentChange(currentPage){\r\n                    this.currentPage=currentPage;\r\n                    this.load();\r\n                }\r\n            }\r\n        }\r\n    </script>\r\n\r\n<style scoped>\r\n    .card-header {\r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n    }\r\n</style>\r\n"],"sourceRoot":""}]}