         </el-table-column>\r\n\r\n\r\n        </el-table>\r\n        <div style=\"margin: 10px 10px\">\r\n            <el-pagination\r\n                    v-model:currentPage=\"currentPage\"\r\n                    :page-sizes=\"[5,10,20]\"\r\n                    :page-size=\"pageSize\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\"\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n            >\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n    <div style=\"margin: 0px 50px\">\r\n        <el-dialog title=\"添加\" v-model=\"dialogVisible\" width=\"30%\">\r\n            <el-form :model=\"user\">\r\n                <el-form-item label=\"课程号\">\r\n                    <el-input v-model=\"user.CID\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"课程名称\">\r\n                    <el-input v-model=\"user.NAME\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"先行课号\">\r\n                    <el-input v-model=\"user.FCID\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"学分\">\r\n                    <el-input v-model=\"user.CREDIT\"></el-input>\r\n                </el-form-item>\r\n               <!-- <el-form-item label=\"出生日期\">\r\n                    <el-input v-model=\"user.BIRTHDAY\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"所在学院\">\r\n                    <el-input v-model=\"user.DNAME\"></el-input>\r\n                </el-form-item>\r\n               <el-form-item label=\"所在年级\">\r\n                    <el-input v-model=\"user.CLASS\"></el-input>\r\n                </el-form-item>-->\r\n            </el-form>\r\n            <span style=\"margin: 0 80px\">\r\n                    <el-button type=\"primary\" @click=\"dialogVisible=false\">确 定</el-button>\r\n                    </span>\r\n        </el-dialog>\r\n    </div>\r\n\r\n    <div style=\"margin: 0px 50px\">\r\n        <el-dialog title=\"选课详情\" v-model=\"dialogVisible1\" width=\"30%\">\r\n            <div>学号：{{'200900130001'}}</div>\r\n            <el-table :data=\"tableData1\">\r\n                <el-table-column\r\n                        label=\"课程号\"\r\n                        style=\"width: 100px\"\r\n                        prop=\"CID\"\r\n                />\r\n                <el-table-column\r\n                        label=\"课程名\"\r\n                        style=\"width: 100px\"\r\n                        prop=\"NAME\"\r\n                />\r\n                <el-table-column\r\n                        label=\"学分\"\r\n                        style=\"width: 100px\"\r\n                        prop=\"CREDIT\"\r\n                />\r\n                <el-table-column  label=\"操作\" width=\"100\">\r\n                    <template #default=\"scope\">\r\n                        <el-button type=\"primary\" style=\"background-color:#F49506\" size=\"mini\" @click=\"deleteCou(scope.row)\">退课</el-button>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n            <div style=\"margin: 10px 10px\">\r\n                <el-pagination\r\n                        v-model:currentPage=\"currentPage1\"\r\n                        :page-sizes=\"[5,10,20]\"\r\n                        :page-size=\"pageSize1\"\r\n                        layout=\"total, sizes, prev, pager, next, jumper\"\r\n                        :total=\"total1\"\r\n                        @size-change=\"handleSizeChange1\"\r\n                        @current-change=\"handleCurrentChange1\"\r\n                >\r\n                </el-pagination>\r\n            </div>\r\n            <span style=\"margin: 0 80px\">\r\n                    <el-button type=\"primary\" @click=\"dialogVisible1=false\">确 定</el-button>\r\n                    </span>\r\n        </el-dialog>\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\n    import request from \"@/utils/request\";\r\n    export default {\r\n        name: \"\",\r\n        data(){\r\n            return{\r\n                dialogVisible:false,\r\n                dialogVisible1:false,\r\n                dialogVisible2:false,\r\n                currentPage:1,\r\n                pageSize:20,\r\n                total:20,\r\n                tableData:[],\r\n                currentPage1:1,\r\n                pageSize1:20,\r\n                total1:20,\r\n                tableData1:[],\r\n                tableData2:[],\r\n                currentPage2:1,\r\n                pageSize2:20,\r\n                user:{\r\n                    CID:'',\r\n                    NAME:'',\r\n                    SEX:'',\r\n                    AGE:'',\r\n                    BIRTHDAY:'',\r\n                    DNAME:'',\r\n                    CLASS:'',\r\n                    FCID:'',\r\n                    CREDIT:''\r\n                }\r\n            }\r\n        },\r\n        created() {\r\n            this.load();\r\n        },\r\n        methods:{\r\n            load(){\r\n                request.get('/api/course/findAll',{\r\n                    params:{\r\n                        pageSize:this.pageSize,\r\n                        currentPage:this.currentPage\r\n                    }\r\n                }).then(res=>{\r\n                    console.log(\"aaa\")\r\n                    this.tableData = res.data.item\r\n                    this.total = res.data.total\r\n                })\r\n            },\r\n\r\n            showGrade(){\r\n                console.log(\"showG\")\r\n                request.get('/api/sc/show',{\r\n                    params:{\r\n                        pageSize:this.pageSize2,\r\n                        currentPage: this.currentPage2\r\n                    }\r\n                }).then(res=>{\r\n                    console.log(\"查看选课！\")\r\n                    this.tableData2 = res.data.item\r\n                    this.total2 = res.data.total\r\n                })\r\n            },\r\n\r\n            showCourse(){\r\n                console.log(\"show\")\r\n                request.get('/api/sc/show',{\r\n                    params:{\r\n                        SID:'200900131001',\r\n                        pageSize:this.pageSize1,\r\n                        currentPage: this.currentPage1\r\n                    }\r\n                }).then(res=>{\r\n                    console.log(\"查看选课！\")\r\n                    this.tableData1 = res.data.item\r\n                    this.total1 = res.data.total\r\n                })\r\n            },\r\n\r\n            selectCou(e){\r\n                console.log(\"selecou\",e.CID)\r\n                request.get('/api/sc/add',{\r\n                    params:{\r\n                    SID:'200900131001',\r\n                    CID: e.CID\r\n                    }\r\n                }).then(res=>{\r\n                    console.log(\"selectCou 执行完毕\")\r\n                })\r\n            },\r\n\r\n            deleteCou(e){\r\n              request.get('/api/sc/delete',{\r\n                  params: {\r\n                      SID: '200900131001',\r\n                      CID: e.CID\r\n                  } }).then(res=>{\r\n                  console.log(\"删除完毕\")\r\n              })\r\n            },\r\n\r\n            handleSizeChange(pageSize)\r\n            {\r\n                this.pageSize=pageSize\r\n                this.load()\r\n            },\r\n            handleCurrentChange(currentPage){\r\n                this.currentPage=currentPage;\r\n                this.load();\r\n            },\r\n\r\n            handleSizeChange1(pageSize1)\r\n            {\r\n                this.pageSize1=pageSize1\r\n                this.showCourse()\r\n            },\r\n            handleCurrentChange1(currentPage1){\r\n                this.currentPage1=currentPage1;\r\n                this.showCourse()\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":""}]}