>用户</el-radio>\n        <el-radio v-model=\"user.identity\" label=\"2\" border>医生</el-radio>\n      </el-form-item>\n        <el-form v-if=\"user.identity===1\" v-model=\"user\" status-icon :rules=\"rules\" ref=\"user\" label-width=\"100px\">\n            <el-form-item label=\"用户名：\" prop=\"name\">\n                <el-input placeholder=\"请输入用户名\" v-model=\"user.userName\" clearable></el-input>\n            </el-form-item>\n            <el-form-item label=\"手机号：\" prop=\"phoneNumber\">\n                <el-input placeholder=\"请输入手机号\" v-model=\"user.phoneNumber\" clearable></el-input>\n            </el-form-item>\n        </el-form>\n      <el-form v-if=\"user.identity===2\" :model=\"doctor\" status-icon :rules=\"rules\" ref=\"doctor\" label-width=\"100px\">\n       <el-form-item label=\"用户名\" prop=\"name\">\n         <el-input placeholder=\"请输入用户名\" v-model=\"doctor.userName\" clearable></el-input>\n       </el-form-item>\n        <el-form-item label=\"电话\" prop=\"phoneNumber\">\n          <el-input placeholder=\"请输入电话号\" v-model=\"doctor.phoneNumber\" clearable></el-input>\n        </el-form-item>\n        <el-form-item label=\"所属医院\" prop=\"hospital\">\n          <el-input placeholder=\"请输入医院名称\" v-model=\"doctor.hospital\" clearable></el-input>\n        </el-form-item>\n        <el-form-item label=\"科室\" prop=\"department\">\n          <el-input placeholder=\"请输入所属科室\" v-model=\"doctor.department\" clearable></el-input>\n        </el-form-item>\n        <el-form-item label=\"职称\" prop=\"title\">\n          <el-select v-model=\"doctor.title\" placeholder=\"请选择\">\n            <el-option\n              v-for=\"item in options1\"\n              :key=\"item.value\"\n              :label=\"item.label\"\n              :value=\"item.value\"\n            >\n            </el-option>\n          </el-select>\n        </el-form-item>\n          <el-form-item label=\"从医年限\" prop=\"year\">\n              <el-input placeholder=\"请输入您的从医年限\" v-model=\"doctor.year\" clearable></el-input>\n          </el-form-item>\n        <el-form-item label=\"专长\" prop=\"major\">\n          <el-input placeholder=\"请输入您的专长\" v-model=\"doctor.major\" clearable></el-input>\n        </el-form-item>\n        <el-form-item label=\"获得荣誉\" prop=\"pride\">\n          <el-input placeholder=\"请输入您获得过的荣誉\" v-model=\"doctor.pride\" clearable></el-input>\n        </el-form-item>\n      </el-form>\n\n      <el-form-item label=\"密码：\" prop=\"password\">\n        <el-input type=\"password\" placeholder=\"请输入密码\" v-model=\"user.password\" show-password autocomplete=\"off\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"确认密码：\" prop=\"confirmPassword\">\n        <el-input type=\"password\" placeholder=\"再输入密码\" v-model=\"user.confirmPassword\" show-password autocomplete=\"off\"></el-input>\n      </el-form-item>\n      <el-button type=\"primary\" @click=\"$router.push('/')\">返回</el-button>\n      <el-button type=\"primary\" class=\"el-button\" @click=\"register\">注册</el-button>\n    </el-form>\n  </div>\n</template>\n<script>\n    import request from \"../utils/request\";\n\n    const Search = require(\"@element-plus/icons/lib/Search\");\n  export default {\n    name: 'uu.vue',\n    data(){\n      var validatePass = (rule, value, callback) =>{\n        if (value === ''){\n          callback(new Error('请输入密码'));\n        }\n        else{\n          if (this.user.confirmPassword !== ''){\n            this.$refs.user.validateField('confirmPassword');\n          }\n          callback();\n        }\n      };\n      var validatePass2 = (rule, value, callback) => {\n        if (value === ''){\n          callback(new Error('请再次输入密码'));\n        } else if (value !== this.user.password) {\n          callback(new Error('两次密码输入不一致！'));\n        }\n        else{\n          callback();\n        }\n      };\n      return{\n        user:{\n        userName:'',    //输入数据\n        phoneNumber:'',\n        password:'',\n        confirmPassword:'',\n        identity:''\n      },\n        options1: [{\n          value: '主任医师',\n          label: '主任医师'\n        },\n          {\n            value: '副主任医师',\n            label: '副主任医师'\n          },\n          {\n            value: '主治医师',\n            label: '主治医师'\n          },\n          {\n            value: '住院医师',\n            label: '住院医师'\n          }\n        ],\n      doctor:{\n          userName: '',\n          phoneNumber: '',\n          hospital: '',\n          department: '',\n          title: [],\n          major: '',\n          pride: '',\n          year:''\n      },\n        /*rules: {\n          password: [\n            { validator: validatePass, trigger: 'blur'}\n          ],\n          confirmPassword: [\n            { validator: validatePass2, trigger: 'blur'}\n          ],\n          phoneNumber: [\n            { required: true, message: '请输入手机号', trigger: 'blur'},\n            { pattern:/^0{0,1}(13[0-9]|15[7-9]|153|156|18[7-9])[0-9]{8}$/, message: \"请输入合法手机号\", trigger: \"blur\" }\n          ],\n          name: [\n            {required: true, message: '姓名不能为空', trigger: 'blur'},\n            {min: 2, max: 4, message: '长度在2到4个字符', trigger: 'blur'}\n          ],\n          title: [\n            { required: true, message: '请选择职位', trigger: 'blur'}\n          ],\n          hospital: [\n            { required: true, message: '所属医院不能为空！', trigger: 'blur'}\n          ],\n          department: [\n            { required: true, message: '所属科室不能为空！', trigger: 'blur'}\n          ],\n          pride: [\n            { required: true, message: '所获荣誉不能为空！', trigger: 'blur'}\n          ],\n          major:[\n            {required: true, message: '专长不能为空！',trigger: 'blur'}\n          ]\n        }*/\n      };\n    },\n    methods: {\n      submitForm(formName) {\n        this.$refs[formName].validate((valid) => {\n          if (valid) {\n            alert('注册成功!');\n          } else {\n            console.log('注册失败，请重新注册!!');\n            return false;\n          }\n        });\n      },\n        register(){\n          if (this.user.identity === \"1\"){\n          request.get('/api/user/sign',{\n              params:{\n                  name: this.user.userName,\n                  phone: this.user.phoneNumber,\n                  password: this.user.password,\n              }\n          }).then(res=>{   //得到后台的反馈结果\n              console.log(res)\n          })\n        }\n          else if (this.user.identity === \"2\"){   //医生注册\n              request.get('api/doctor/sign',{\n                  params:{\n                     name: this.doctor.userName,\n                     phone: this.doctor.phoneNumber,\n                     password: this.user.password,\n                     hospital: this.doctor.hospital,\n                     department: this.doctor.department,\n                     title: this.doctor.title,\n                     major: this.doctor.major,\n                     pride: this.doctor.pride,\n                     year: this.doctor.year\n                  }\n              }).then(res=>{\n                  sessionStorage.setItem(\"doctor\",JSON.stringify(this.doctor));\n                  var d = JSON.parse(sessionStorage.getItem(\"doctor\"));\n                  console.log(d);\n                  console.log(res)\n              })\n          }\n      }\n    }\n  }\n</script>\n<style>\n  .el-button{\n    background-color: deepskyblue;\n  }\n</style>\n"],"sourceRoot":""}]}