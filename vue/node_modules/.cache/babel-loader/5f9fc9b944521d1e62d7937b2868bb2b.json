">\n      <el-form-item label=\"请选择身份:\">\n    <el-radio v-model=\"User.identity\" label=\"1\" border>用户</el-radio>\n    <el-radio v-model=\"User.identity\" label=\"2\" border>医生</el-radio>\n      </el-form-item>\n      <el-form-item label=\"用户名：\" prop=\"username\">\n    <el-input placeholder=\"请输入用户名\" v-model=\"User.username\" clearable>\n    </el-input>\n      </el-form-item>\n      <el-form-item label=\"手机号：\" prop=\"phoneNumber\">\n    <el-input placeholder=\"请输入手机号\" v-model=\"User.phoneNumber\" clearable>\n    </el-input>\n      </el-form-item>\n      <el-form-item label=\"密码：\" prop=\"password\">\n    <el-input placeholder=\"请输入密码\" v-model=\"User.password\" show-password></el-input>\n      </el-form-item>\n  <el-button class=\"el-button\" type=\"primary\" @click=\"login\">登录</el-button>\n  <el-button type=\"primary\" @click=\"$router.push('/SignIn')\">注册</el-button>\n    </el-form>\n  </div>\n</template>\n<script>\n\n  import request from \"@/utils/request\";\n\n  export default {\n      name: 'Login.vue',\n      data() {\n          return {\n              User: {},\n              /* rules: {\n                  phoneNumber: [\n                      {required: true, message: '请输入手机号', trigger: 'blur'},\n                      {\n                          pattern: /^0{0,1}(13[0-9]|15[7-9]|153|156|18[7-9])[0-9]{8}$/,\n                          message: \"请输入合法手机号\",\n                          trigger: \"blur\"\n                      }\n                  ],\n                  username: [\n                      {required: true, message: '姓名不能为空', trigger: 'blur'},\n                      {min: 2, max: 4, message: '长度在2到4个字符', trigger: 'blur'}\n                  ],\n                  password: [\n                      {required: true, message: '密码不能为空', transform: value => value, trigger: 'blur'},\n                      {\n                          type: 'string',\n                          message: '请输入不包含空格的字符',\n                          trigger: 'blur',\n                          transform(value) {\n                              if (value && value.indexOf(' ') === -1) {\n                                  return value\n                              } else {\n                                  return false\n                              }\n                          }\n                      },\n                      {\n                          trigger: 'blur',\n                          validator: (rule, value, callback) => {\n                              var passwordreg = /(?=.*\\d)(?=.*[a-zA-Z])(?=.*[^a-zA-Z0-9]).{6,16}/\n                              if (!passwordreg.test(value)) {\n                                  callback(new Error('密码必须由数字、字母、特殊字符组合,请输入6-16位'))\n                              } else {\n                                  callback()\n                              }\n                          }\n                      }\n\n                  ]\n              }\n          }*/\n          }\n      },\n      methods: {\n          login() {\n              if (this.User.identity === \"1\"){    //字符串  患者\n              request.get('/api/user/login',{\n                  params : {\n                    name : this.User.username,\n                    phone: this.User.phoneNumber,\n                    password: this.User.password\n                  }\n                  }\n              ).then(res => {   //得到后台的反馈结果\n                  alert(res.data.message)\n                  if ((res.data.code !==2) && (res.data.code !==3)){\n                      localStorage.setItem(\"user\",this.User.username);\n                      this.$router.push('/pM')\n                  }\n               //  var u = sessionStorage.getItem(\"user\");\n                //  u = JSON.parse(u);\n                //  console.log(u.username)\n                  console.log(res)\n               //   console.log(u)\n              })\n          }\n              else if (this.User.identity === \"2\"){   //医生\n                  request.get('/api/doctor/login',{\n                      params:{\n                          name: this.User.username,\n                          phone: this.User.phoneNumber,\n                          password: this.User.password,\n                      }\n                  }).then(res=>{\n                      alert(res.data.message)\n                      if (res.data.code === 0){\n                          sessionStorage.setItem(\"user\",this.User.username);   //存储整个对象\n                          this.$router.push('/dM')\n                      }\n                      console.log(res)\n                  })\n              }\n          }\n      }\n  }\n</script>\n<style>\n.login{\n  width:600px;\n  height:500px;\n  line-height: 20px;\n  margin: 215px auto;\n  margin-right: 200px;\n  overflow: hidden;\n  padding-top: 10px;\n  margin-bottom: 20px;\n}\n.el-button{\n background-color: deepskyblue;\n}\n</style>\n"],"sourceRoot":""}]}