r>\r\n    <el-form :model=\"question\" label-position=\"left\">\r\n        <el-card style=\"width: 1000px;margin-top: 30px;margin-left: 250px;background-color: deepskyblue\"\r\n                 shadow=\"never\"\r\n        >\r\n            <template #header>\r\n                <div class=\"card-header\">\r\n                    <span>发布人</span>\r\n                    <span>日期</span>\r\n                    <span>收藏量</span>\r\n                </div>\r\n            </template>\r\n            <div>问题详情</div>\r\n            <div style=\"margin: 30px 250px\">\r\n                <el-dialog title=\"回答\" v-model=\"dialogVisible\" width=\"30%\">\r\n                    <el-form :model=\"form\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"120px\" action=\"post\">\r\n                        <el-form-item label=\"在这里写下你的回答吧！\" prop=\"answers\">\r\n                            <el-input v-model=\"form.answers\" style=\"width: 100%\" type=\"textarea\" :rows=\"5\"></el-input>\r\n                        </el-form-item>\r\n                    </el-form>\r\n                    <span style=\"margin: 0 80px\">\r\n    <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n  </span>\r\n                </el-dialog>\r\n            </div>\r\n            <div style=\"margin: 30px 250px\">\r\n                <el-dialog  v-model=\"dialogVisible1\" width=\"50%\">\r\n                    <el-form :model=\"forms\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"120px\" style=\"height: 500px\" action=\"post\">\r\n                        <el-card style=\"height:180px; background-color: deepskyblue\"\r\n                                 shadow=\"never\"\r\n                        >\r\n                            <template #header>\r\n                                <div class=\"card-header\">\r\n                                    <span>你好吗？</span>\r\n                                    <span>日期</span>\r\n                        