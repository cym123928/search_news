put v-model=\"doctor.hospital\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"手机号\" prop=\"phoneNumber\">\r\n                        <el-input v-model=\"doctor.phoneNumber\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"所属医院\" prop=\"hospital\">\r\n                        <el-input v-model=\"doctor.hospital\">\r\n                        </el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"所属科室\" prop=\"department\">\r\n                        <el-input v-model=\"doctor.department\">\r\n                        </el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"职称\" prop=\"title\">\r\n                        <el-select v-model=\"doctor.title\" placeholder=\"请选择\">\r\n                            <el-option\r\n                                    v-for=\"item in options2\"\r\n                                    :key=\"item.value\"\r\n                                    :label=\"item.label\"\r\n                                    :value=\"item.value\"\r\n                            >\r\n                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"专长\" prop=\"major\">\r\n                        <el-input v-model=\"doctor.major\">\r\n                        </el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"荣誉\" prop=\"pride\">\r\n                        <el-input v-model=\"doctor.pride\">\r\n                        </el-input>\r\n                    </el-form-item>\r\n                </el-form>\r\n                <span style=\"margin: 0 80px\">\r\n    <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"\">确 定</el-button>\r\n  </span>\r\n            </el-dialog>\r\n        </div>\r\n        <div style=\"width: 100px;\">\r\n            <el-dropdown>\r\n  <span class=\"el-dropdown-link\">\r\n      {{na}}\r\n     <el-icon class=\"el-icon--right\">\r\n        <arrow-down />\r\n      </el-icon>\r\n    </span>\r\n                <template #dropdown>\r\n                    <el-dropdown-menu slot=\"dropdown\">\r\n                        <el-dropdown-item @click=\"dialogVisible=true\">个人信息</el-dropdown-item>\r\n                        <el-dropdown-item>退出系统</el-dropdown-item>\r\n                    </el-dropdown-menu>\r\n                </template>\r\n            </el-dropdown>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\n    export default {\r\n        name: \"dM\",\r\n        data(){\r\n            var d = JSON.parse(sessionStorage.getItem(\"doctor\"));\r\n            var p = JSON.parse(sessionStorage.getItem(\"dp\"));\r\n          return{\r\n              na:p.userName,    //显示用户名\r\n              options2: [{\r\n                  value: '主任医师',\r\n                  label: '主任医师'\r\n              },\r\n                  {\r\n                      value: '副主任医师',\r\n                      label: '副主任医师'\r\n                  },\r\n                  {\r\n                      value: '主治医师',\r\n                      label: '主治医师'\r\n                  },\r\n                  {\r\n                      value: '住院医师',\r\n                      label: '住院医师'\r\n                  }\r\n              ],\r\n              dialogVisible:false,\r\n              doctor:{\r\n                  name:p.userName,\r\n                  hospital:d.hospital,\r\n                  phoneNumber:p.phoneNumber,\r\n                  department:d.department,\r\n                  title:d.title,   //职称  多选\r\n                  major:d.major,\r\n                  pride:d.pride,\r\n                  year:d.year,\r\n              },\r\n              rules: {\r\n                  name: [\r\n                      {required: true, message: '姓名不能为空', trigger: 'blur'},\r\n              