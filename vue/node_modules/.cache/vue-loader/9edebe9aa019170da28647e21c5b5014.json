style=\"width: 200px;min-height:calc(100vh - 50px);margin-top: 50px;margin-left: -200px\"\r\n                    :default-active=\"$router.path\"\r\n                    router\r\n                    @open=\"handleOpen\"\r\n                    @close=\"handleClose\"\r\n                    class=\"el-menu-vertical-demo\"\r\n                    background-color=\"#5096DE\"\r\n            >\r\n                <el-sub-menu index=\"1\">\r\n                    <template #title>\r\n                        <el-icon><location /></el-icon>\r\n                        <span>问题专区</span>\r\n                    </template>\r\n                    <el-menu-item index=\"/recommendForPatient\">推荐问题</el-menu-item>\r\n                    <el-menu-item index=\"/question\">所有问题</el-menu-item>\r\n                    <el-menu-item index=\"/rQuestion\">已答问题</el-menu-item>\r\n                    <el-menu-item index=\"/postedP\">已发布问题</el-menu-item>\r\n                    <el-menu-item index=\"/collectP\">问题收藏</el-menu-item>\r\n                </el-sub-menu>\r\n                <el-menu-item index=\"/speechPatient\">\r\n                    <el-icon><document /></el-icon>\r\n                    <span>讲座预约</span>\r\n                </el-menu-item>\r\n            </el-menu>\r\n        </div>\r\n        <div style=\"flex: 1;\">\r\n            <router-view></router-view>\r\n        </div>\r\n        <div style=\"margin: 0px 50px\">\r\n            <el-dialog title=\"个人信息\" v-model=\"dialogVisible\" width=\"30%\">\r\n                <el-form size=\"medium\" :model=\"patient\" ref=\"patient\" :rules=\"rules\"\r\n                         style=\"margin-top: 15px\"\r\n                >\r\n                    <el-form-item label=\"姓名\" prop=\"name\">\r\n                        <el-input v-model=\"patient.name\"></el-input>\r\n                    </el-form-item>\r\n                  <!--  <el-form-item label=\"年龄\" prop=\"age\">\r\n                        <el-input v-model.number=\"patient.age\"></el-input>\r\n                    </el-form-item>-->\r\n                    <el-form-item label=\"手机号\" prop=\"phoneNumber\">\r\n                        <el-input v-model=\"patient.phoneNumber\"></el-input>\r\n                    </el-form-item>\r\n                   <!-- <el-form-item label=\"邮箱地址\" prop=\"emailAddress\">\r\n                        <el-input v-model=\"patient.emailAddress\">\r\n                            <template slot=\"append\">.com</template>\r\n                        </el-input>\r\n                    </el-form-item>-->\r\n                </el-form>\r\n                <span style=\"margin: 0 80px\">\r\n    <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"\">确 定</el-button>\r\n  </span>\r\n            </el-dialog>\r\n        </div>\r\n        <div style=\"width: 100px;\">\r\n            <el-dropdown>\r\n  <span class=\"el-dropdown-link\" >\r\n         {{user}}\r\n     <el-icon class=\"el-icon--right\">\r\n        <arrow-down />\r\n      </el-icon>\r\n    </span>\r\n                <template #dropdown>\r\n                    <el-dropdown-menu slot=\"dropdown\">\r\n                        <el-dropdown-item @click=\"dialogVisible=true\">个人信息</el-dropdown-item>\r\n                        <el-dropdown-item>退出系统</el-dropdown-item>\r\n                    </el-dropdown-menu>\r\n                </template>\r\n            </el-dropdown>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\n    export default {\r\n        name: \"pM\",\r\n        data(){\r\n          return{\r\n           user:'', //localStorage.getItem(\"username\") ,   //得到sessionStorage的值\r\n              patient:{\r\n                  name: '',//localStorage.getItem(\"username\"),\r\n                  phoneNumber:'', //localStorage.getItem(\"phone\"),\r\n              },\r\n              dialogVisible: false,\r\n              rules: {\r\n                  name: [\r\n                      {required: true, message: '姓名不能为空', trigger: 'blur'},\r\n                      {min: 2, max: 4, message: '长度在2到4个字符', trigger: 'blur'}\r\n                  ],\r\n                  age: [\r\n                      {required: true, message: '年龄不能为空',trigger: 'blur'},\r\n                      {type: 'number', message: '年龄不是数字值', trigger: 'blur'}\r\n                  ],\r\n                  phoneNumber: [\r\n                      { required: true, message: '请输入手机号', trigger: 'blur'},\r\n                      { pattern:/^0{0,1}(13[0-9]|15[7-9]|153|156|18[7-9])[0-9]{8}$/, message: \"请输入合法手机号\", trigger: \"blur\" }\r\n                  ],\r\n                  emailAddress: [\r\n                      { required: true, message: \"请输入邮箱\", trigger: \"blur\" },\r\n                      { pattern:/^([0-9A-Za-z\\-_\\.]+)@([0-9a-z]+\\.[a-z]{2,3}(\\.[a-z]{2})?)$/g, message: \"请输入正确的邮箱\", trigger: \"blur\"}\r\n                  ]\r\n              }\r\n          }\r\n        },\r\n        methods:{\r\n            handleOpen(key,keyPath){\r\n                console.log(key,keyPath);\r\n            },\r\n            handleClose(key,keyPath){\r\n                console.log(key,keyPath);\r\n            },\r\n            goto(path){\r\n                this.$router.replace(path);\r\n            },\r\n\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .el-form-item{\r\n        width: 400px;\r\n    }\r\n</style>\r\n"]}]}